{
    "name": "ChronoGraph : Time Tracker",
    "description": "Beautiful and minimal New Tab page that helps you to improve productivity by tracking time spent on websites",
    "version": "1.0.0",
    "manifest_version": 2,
    "author": "Dhruv Kothari",

    "icons": { 
        "16": "pics/icon16.png",
        "48": "pics/icon48.png",
       "128": "pics/icon128.png" 
    },

    "background": {
        "scripts": ["background.js"]
    },

    "browser_action": {
        "default_title": "ChronoGraph",
        "default_popup": "popup.html"
    },

    "chrome_url_overrides": {
        "newtab" : "index.html"
    },

    "permissions": ["tabs", "storage"],

    "content_security_policy":"script-src 'self' https://apis.google.com; object-src 'self'"

}